{
  "name": "vue-box",
  "version": "1.0.2",
  "description": "A Small module that pull vue components into a simple testable environment for methods, computed props, and watched functionality",
  "main": "index.js",
  "scripts": {
    "test:multi": "tape tests/component-import/tests/test.js | tap-spec",
    "test:single": "tape tests/single-component/tests/test.js | tap-spec",
    "test": "tape 'tests/**/test.js' | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/vue-box.git"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/vue-box/issues"
  },
  "keywords": [
    "vue",
    "testing",
    "unit",
    "tests",
    "vue-box",
    "components"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "homepage": "https://github.com/dhershman1/vue-box#readme",
  "devDependencies": {
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "underscore": "^1.8.3"
  },
  "dependencies": {
    "rollup": "^0.45.2",
    "rollup-plugin-multi-entry": "^2.0.1"
  }
}
